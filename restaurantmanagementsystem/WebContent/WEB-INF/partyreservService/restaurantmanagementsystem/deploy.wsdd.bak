<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from partyreservService WSDL service -->

  <service name="partyreserv" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://restaurantmanagementsystem"/>
      <parameter name="wsdlServiceElement" value="partyreservService"/>
      <parameter name="schemaQualified" value="http://restaurantmanagementsystem"/>
      <parameter name="wsdlServicePort" value="partyreserv"/>
      <parameter name="className" value="restaurantmanagementsystem.PartyreservSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="partyreserv"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addpartyres" qname="operNS:addpartyres" xmlns:operNS="http://restaurantmanagementsystem" soapAction="" >
        <parameter qname="pns:partyid" xmlns:pns="http://restaurantmanagementsystem" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Username" xmlns:pns="http://restaurantmanagementsystem" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:ContactNo" xmlns:pns="http://restaurantmanagementsystem" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:arrivaldate" xmlns:pns="http://restaurantmanagementsystem" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Arrivaltime" xmlns:pns="http://restaurantmanagementsystem" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:decorationtype" xmlns:pns="http://restaurantmanagementsystem" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:totalmembers" xmlns:pns="http://restaurantmanagementsystem" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="addpartyres"/>

  </service>
</deployment>
