<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from restmenuService WSDL service -->

  <service name="restmenu" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://restaurantmanagementsystem"/>
      <parameter name="wsdlServiceElement" value="restmenuService"/>
      <parameter name="schemaQualified" value="http://restaurantmanagementsystem"/>
      <parameter name="wsdlServicePort" value="restmenu"/>
      <parameter name="className" value="restaurantmanagementsystem.RestmenuSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="restmenu"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="menu" qname="operNS:menu" xmlns:operNS="http://restaurantmanagementsystem" returnQName="retNS:menuReturn" xmlns:retNS="http://restaurantmanagementsystem" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:menuid" xmlns:pns="http://restaurantmanagementsystem" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="menu"/>

  </service>
</deployment>
